  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach Debugger to Next App",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Next App (Dev)",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["run", "dev"],
      "port": 9229
    }
  ]
}
